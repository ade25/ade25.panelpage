<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="ade25.panelpage">
<body>

<metal:main fill-slot="main">
  <metal:content-core define-macro="main">

    <div class="panelpage-editor"
      tal:define="parent_info view/parent_info">
        <div class="ppe-header">
            <a class="ppe-close pull-right"
                tal:attributes="href string:${parent_info/url}/@@panelpage-editor">
                &times;
            </a>
            <div class="ppe-header-title" i18n:translate="">Page Explorer</div>
        </div>
        <div class="ppe-subtitle">
          <span tal:replace="parent_info/title" /> /
          <span tal:replace="context/Title" />
        </div>
        <div class="ppe-section" id="content-core">
            <div tal:condition="view/query">
              <tal:block tal:repeat="item view/dynamic_contents">
                <h5 tal:content="item/Title" />
              </tal:block>
            </div>
            <div tal:replace="structure view/render_item" />
        </div>
    </div>

  </metal:content-core>
</metal:main>

</body>
</html>
